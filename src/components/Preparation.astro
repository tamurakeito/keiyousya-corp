---
import { Image } from 'astro:assets';
import Prince from '@/assets/images/prince.svg';
---

<div class="flex flex-col justify-center items-center h-[calc(100vh-110px)] gap-[16px]">
  <Image src={Prince} alt="Prince" class="w-[60px] h-[60px] animate-spin" style="animation-duration: 10s;" />
  <p class="text-[14px] text-gray-900 animate-pulse" style="animation-duration: 1s;">Preparing...</p>
</div>